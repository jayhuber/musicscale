<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="question/type/musicscale/db" VERSION="20121010" COMMENT="XMLDB file for this question type">
  <TABLES>
    <TABLE NAME="question_musicscale" COMMENT="Options for musicscale questions">
      <FIELDS>
	  
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true"
               ENUM="false" NEXT="questionid"/>
			   
        <FIELD NAME="questionid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0"
               SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="includeks"/>
			   
        <FIELD NAME="includeks" TYPE="char" LENGTH="1" NOTNULL="true" UNSIGNED="false"
               SEQUENCE="false" ENUM="false" PREVIOUS="questionid" NEXT="modescale"/>
			   
		<FIELD NAME="modescale" TYPE="char" LENGTH="2" NOTNULL="true" UNSIGNED="false"
               SEQUENCE="false" ENUM="false" PREVIOUS="includeks" NEXT="orignoteletter"/>
			   
        <FIELD NAME="orignoteletter" TYPE="char" LENGTH="1" NOTNULL="true" UNSIGNED="false"
               SEQUENCE="false" ENUM="false" PREVIOUS="modescale" NEXT="orignoteaccidental"/>
	
		<FIELD NAME="orignoteaccidental" TYPE="char" LENGTH="1" NOTNULL="true" UNSIGNED="false"
               SEQUENCE="false" ENUM="false" PREVIOUS="orignoteletter" NEXT="orignoteregister"/>
			   
        <FIELD NAME="orignoteregister" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true"
               SEQUENCE="false" ENUM="false" PREVIOUS="orignoteaccidental" />
			   
      </FIELDS>
	  
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="questionid"/>
        <KEY NAME="questionid" TYPE="foreign" FIELDS="questionid" REFTABLE="questions" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>